<% layout('./layouts/admin') %>
    <div class="row">
        <div class="col-sm-12">
            <h4 class="page-title">Payment Manager</h4>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="<%= dashboard %>">Dashboard</a></li>
                <li class="breadcrumb-item active">Payment Manager</li>
            </ol>
        </div>
    </div>

    <div class="row">
        <div class="col-12">
            <div class="card-box" style="overflow:scroll;">
                <h4 class="m-t-0 m-b-20 header-title">All Payments</h4>

                <table id="payment-datatable" class="table table-bordered table-service" style="width: 100% !important;">
                    <thead>
                        <tr>
                            <th>S. No</th>
                            <th>Req Id</th>
                            <th>User</th>
                            <th>Shop</th>
                            <th>Amount($)</th>
                            <th>Created At</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>