<% layout('./layouts/admin') %>
    <div class="row">
        <div class="col-sm-12">
            <h4 class="page-title">Report Manager</h4>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="<%= dashboard %>">Dashboard</a></li>
                <li class="breadcrumb-item active">Report Manager</li>
            </ol>
        </div>
    </div>

    <div class="row">
        <div class="col-12">
            <div class="card-box" style="overflow:scroll;">
                <h4 class="m-t-0 m-b-20 header-title">All Reports</h4>

                <table id="report-datatable" class="table table-bordered table-service" style="width: 100% !important;">
                    <thead>
                        <tr>
                            <th>S No</th>
                            <th>Reported By</th>
                            <th>Reported To</th>
                            <th>Reported on</th>
                            <th>Created At</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>