    <script src="/plugins/jquery-validation/js/jquery.validate.min.js"></script>
    <script src="/plugins/moment/moment.js"></script>
    <script src="/plugins/bootstrap-sweetalert/sweet-alert.min.js"></script>
    <script src="/plugins/waypoints/lib/jquery.waypoints.min.js"></script>
    <script src="/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="/plugins/datatables/dataTables.bootstrap4.min.js"></script>
    <script type="text/javascript" src="/plugins/notifyjs/js/notify.js"></script>
    <script type="text/javascript" src="/plugins/notifications/notify-metro.js"></script>

    <script src="/plugins/datatables/pdfmake.min.js"></script>

    <script src="/js/datatables.js"></script>
    <script src="/js/jquery.app.js"></script>
    <script src="/js/project.js"></script>
    <script src="/js/jquery.core.js"></script>
    <script src="/js/fastclick.js"></script>
    <script src="/js/jquery.slimscroll.js"></script>
    <script src="/js/custom.js"></script>
